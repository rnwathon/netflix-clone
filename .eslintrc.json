{
  "env": {
    "jest": true,
    "browser": true,
    "es2021": true
  },
  "plugins": ["simple-import-sort"],
  "extends": ["next/core-web-vitals", "prettier"],
  "rules": {
    "no-unused-vars": 0,
    "no-console": "warn",

    "simple-import-sort/exports": "warn",
    "simple-import-sort/imports": "warn"
  }
}
